{
    "name": "website",
    "root": "apps/website",
    "sourceRoot": "apps/website",
    "projectType": "application",
    "targets": {
        "astro": {
            "executor": "nx:run-commands",
            "options": {
                "command": "astro",
                "cwd": "apps/website"
            }
        },
        "dev": {
            "executor": "nx:run-commands",
            "options": {
                "command": "astro dev",
                "cwd": "apps/website"
            }
        },
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "astro build",
                "cwd": "apps/website",
                "outputs": [
                    "dist/apps/website"
                ]
            }
        },
        "preview": {
            "executor": "nx:run-commands",
            "options": {
                "command": "astro preview",
                "cwd": "apps/website"
            }
        }
    },
    "tags": []
}